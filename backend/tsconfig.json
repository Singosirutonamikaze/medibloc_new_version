{
  "compilerOptions": {
    "target": "es2020",
    "module": "node16",
    "lib": ["ES2020", "ES2021"],
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"]
  },
  "include": ["src", "prisma"]
}
